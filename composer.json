{
  "name": "bfst/bfst",
  "autoload": {
    "psr-4": {
      "": "app/BFST/classes/"
    },
    "files": [
      "app/BFST/classes/Volcano/helpers.php"
    ]
  },
  "autoload-dev": {
    "\\BFST\\Units": "tests/units"
  },
  "require": {
    "ext-redis": "*",
    "ext-pdo": "*",
    "vlucas/phpdotenv": "^5.5",
    "volcano/routing": "^0.1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5"
  },
  "scripts": {
    "test": [
      "@phpunit",
      "@phpcs",
      "@phpstan"
    ],
    "phpunit": "phpunit",
    "phpcbf": "tools/php_codesniffer/vendor/bin/phpcbf --standard=PSR12 app",
    "phpcs": "tools/php_codesniffer/vendor/bin/phpcs --standard=PSR12 app",
    "phpstan": "tools/phpstan/vendor/bin/phpstan --memory-limit=-1"
  },
  "config": {
    "sort-packages": true
  }
}
