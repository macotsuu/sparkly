{
  "name": "sparkly/sparkly",
  "autoload": {
    "psr-4": {
      "Sparkly\\": "src/apps",
      "Sparkly\\Modules\\": "src/modules",
      "Sparkly\\System\\": "src/system"
    }
  },
  "require": {
    "vlucas/phpdotenv": "^5.5"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5"
  },
  "config": {
    "sort-packages": true,
    "optimize-autoloader": true
  },
  "minimum-stability": "dev"
}
