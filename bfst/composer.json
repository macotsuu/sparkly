{
  "name": "bfst/bfst",
  "autoload": {
    "psr-4": {
      "": "app/BFST/classes/"
    },
    "files": [
      "app/BFST/classes/Volcano/functions/common.php",
      "app/BFST/classes/Volcano/functions/url.php"
    ]
  },
  "require": {
    "vlucas/phpdotenv": "^5.5",
    "ext-redis": "*",
    "ext-pdo": "*",
    "volcano/routing": "^0.1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5",
    "phpstan/phpstan": "^1.9",
    "squizlabs/php_codesniffer": "3.*"
  },
  "scripts": {
    "test": [
      "@phpunit",
      "@phpcs",
      "@phpstan"
    ],
    "phpunit": "phpunit",
    "phpcs": "phpcs",
    "phpstan": "phpstan --memory-limit=-1"
  },
  "config": {
    "sort-packages": true
  }
}
